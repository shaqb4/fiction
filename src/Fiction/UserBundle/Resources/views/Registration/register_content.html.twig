{% trans_default_domain 'FOSUserBundle' %}

<form role="form" action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
    {{ form_widget(form) }}
    <button type="submit" class="btn btn-primary" value="{{ 'registration.submit'|trans }}">
    	{{ 'registration.submit'|trans }}
    </button>
</form>
