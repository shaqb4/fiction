{% trans_default_domain 'FOSUserBundle' %}

<form role="form" action="{{ path('fos_user_resetting_reset', {'token': token}) }}" {{ form_enctype(form) }} method="POST" class="fos_user_resetting_reset">
    {{ form_widget(form) }}
    <button type="submit" class="btn btn-primary" value="{{ 'resetting.reset.submit'|trans }}">
    	{{ 'resetting.reset.submit'|trans }}
    </button>
</form> 
